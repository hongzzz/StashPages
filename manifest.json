{
    "name": "Stash Pages",
    "version": "1.0",
    "description": "Stash Pages",
    "homepage_url": "https://github.com/hongzzz",
    "permissions": [
        "storage",
        "tabs",
        "unlimitedStorage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "pic.png"
    },
    "icons": {
        "16": "pic.png",
        "32": "pic.png",
        "48": "pic.png",
        "128": "pic.png"
    },
    "manifest_version": 2
}
